{
  "name": "Jupyter Notebook Environment",
  "image": "mcr.microsoft.com/vscode/devcontainers/python:3.10",
  "features": {
    "python": "3.10"
  },
  "postCreateCommand": "pip install numpy pandas matplotlib jupyter",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter"
      ]
    }
  }
}
